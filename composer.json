{
    "name": "jorisvanw/deepl-laravel",
    "description": "DeepL API client wrapper for Laravel",
    "keywords": ["deepl", "deeply", "laravel", "translation", "translate", "language", "api", "client", "english", "german", "french"],
    "homepage": "https://github.com/JorisvanW/deepl-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Joris v. Willigen",
            "email": "me@jorisvanw.me"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0",
        "jorisvanw/deepl-api-php": "^0.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "JorisvanW\\DeepL\\Laravel\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "JorisvanW\\DeepL\\Laravel\\DeepLServiceProvider"
            ],
            "aliases": {
                "DeepL": "JorisvanW\\DeepL\\Laravel\\Facade"
            }
        }
    }
}
